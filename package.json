{
  "name": "@rharel/webext-private-bookmarks",
  "version": "0.1.17.1a",
  "description": "WebExtension that enables a special password-protected bookmark folder.",
  "directories": {
    "test": "tests"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "requirejs": "^2.3.5",
    "web-ext": "^2.8.0"
  },
  "scripts": {
    "build": "web-ext build --source-dir ./sources/ --artifacts-dir ./distribution/ --overwrite-dest",
    "lint": "web-ext lint --source-dir ./sources/",
    "start:firefox": "web-ext run --source-dir ./sources/ --start-url about:addons --browser-console --verbose",
    "start:firefoxDev": "web-ext run --source-dir ./sources/ --start-url about:addons --browser-console --verbose --firefox firefoxdeveloperedition"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rharel/webext-private-bookmarks.git"
  },
  "keywords": [
    "WebExtension",
    "privacy",
    "bookmarks",
    "password"
  ],
  "author": "Raoul Harel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rharel/webext-private-bookmarks/issues"
  },
  "homepage": "https://github.com/rharel/webext-private-bookmarks#readme"
}
